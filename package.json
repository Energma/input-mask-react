{
  "name": "@energma/workspace",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "pnpm --filter test-app dev",
    "test-watch": "pnpm --filter ./lib test",
    "test": true,
    "build": "pnpm --filter ./lib build",
    "run-tests": "pnpm test",
    "build-package": true,
    "publish-package": true,
    "clean-all": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +; find . -name 'dist' -type d -prune -exec rm -rf '{}' +"
  }
}